{
  "name": "ts-time-utils",
  "version": "3.0.0",
  "description": "A comprehensive TypeScript utility library for time, dates, durations, and calendar operations with full tree-shaking support",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./format": {
      "import": "./dist/esm/format.js",
      "require": "./dist/format.js",
      "types": "./dist/format.d.ts"
    },
    "./calculate": {
      "import": "./dist/esm/calculate.js",
      "require": "./dist/calculate.js",
      "types": "./dist/calculate.d.ts"
    },
    "./validate": {
      "import": "./dist/esm/validate.js",
      "require": "./dist/validate.js",
      "types": "./dist/validate.d.ts"
    },
    "./constants": {
      "import": "./dist/esm/constants.js",
      "require": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    },
    "./age": {
      "import": "./dist/esm/age.js",
      "require": "./dist/age.js",
      "types": "./dist/age.d.ts"
    },
    "./calendar": {
      "import": "./dist/esm/calendar.js",
      "require": "./dist/calendar.js",
      "types": "./dist/calendar.d.ts"
    },
    "./parse": {
      "import": "./dist/esm/parse.js",
      "require": "./dist/parse.js",
      "types": "./dist/parse.d.ts"
    },
    "./performance": {
      "import": "./dist/esm/performance.js",
      "require": "./dist/performance.js",
      "types": "./dist/performance.d.ts"
    },
    "./interval": {
      "import": "./dist/esm/interval.js",
      "require": "./dist/interval.js",
      "types": "./dist/interval.d.ts"
    },
    "./timezone": {
      "import": "./dist/esm/timezone.js",
      "require": "./dist/timezone.js",
      "types": "./dist/timezone.d.ts"
    },
    "./workingHours": {
      "import": "./dist/esm/workingHours.js",
      "require": "./dist/workingHours.js",
      "types": "./dist/workingHours.d.ts"
    },
    "./rangePresets": {
      "import": "./dist/esm/rangePresets.js",
      "require": "./dist/rangePresets.js",
      "types": "./dist/rangePresets.d.ts"
    },
    "./duration": {
      "import": "./dist/esm/duration.js",
      "require": "./dist/duration.js",
      "types": "./dist/duration.d.ts"
    },
    "./serialize": {
      "import": "./dist/esm/serialize.js",
      "require": "./dist/serialize.js",
      "types": "./dist/serialize.d.ts"
    },
    "./locale": {
      "import": "./dist/esm/locale.js",
      "require": "./dist/locale.js",
      "types": "./dist/locale.d.ts"
    },
    "./recurrence": {
      "import": "./dist/esm/recurrence.js",
      "require": "./dist/recurrence.js",
      "types": "./dist/recurrence.d.ts"
    },
    "./countdown": {
      "import": "./dist/esm/countdown.js",
      "require": "./dist/countdown.js",
      "types": "./dist/countdown.d.ts"
    },
    "./dateRange": {
      "import": "./dist/esm/dateRange.js",
      "require": "./dist/dateRange.js",
      "types": "./dist/dateRange.d.ts"
    },
    "./naturalLanguage": {
      "import": "./dist/esm/naturalLanguage.js",
      "require": "./dist/naturalLanguage.js",
      "types": "./dist/naturalLanguage.d.ts"
    },
    "./cron": {
      "import": "./dist/esm/cron.js",
      "require": "./dist/cron.js",
      "types": "./dist/cron.d.ts"
    },
    "./fiscal": {
      "import": "./dist/esm/fiscal.js",
      "require": "./dist/fiscal.js",
      "types": "./dist/fiscal.d.ts"
    },
    "./compare": {
      "import": "./dist/esm/compare.js",
      "require": "./dist/compare.js",
      "types": "./dist/compare.d.ts"
    },
    "./iterate": {
      "import": "./dist/esm/iterate.js",
      "require": "./dist/iterate.js",
      "types": "./dist/iterate.d.ts"
    },
    "./holidays": {
      "import": "./dist/esm/holidays.js",
      "require": "./dist/holidays.js",
      "types": "./dist/holidays.d.ts"
    },
    "./chain": {
      "import": "./dist/esm/chain.js",
      "require": "./dist/chain.js",
      "types": "./dist/chain.d.ts"
    },
    "./plugins": {
      "import": "./dist/esm/plugins.js",
      "require": "./dist/plugins.js",
      "types": "./dist/plugins.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "size": "size-limit",
    "size:check": "size-limit --json > docs/bundle-sizes.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "time",
    "date",
    "duration",
    "calendar",
    "utilities",
    "formatting",
    "typescript",
    "tree-shaking",
    "timezone",
    "working-hours",
    "intervals",
    "age-calculation",
    "business-days",
    "date-parsing",
    "serialization",
    "internationalization",
    "i18n",
    "locale",
    "relative-time",
    "recurrence",
    "recurring-events",
    "rrule",
    "countdown",
    "timer",
    "date-range",
    "natural-language",
    "nlp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/file": "^12.0.0",
    "size-limit": "^12.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hatefrad/ts-time-utils.git"
  },
  "homepage": "https://github.com/hatefrad/ts-time-utils#readme",
  "bugs": {
    "url": "https://github.com/hatefrad/ts-time-utils/issues"
  }
}
